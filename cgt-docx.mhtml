        emit SanctionListUpdated(_address, _status);
    }

    function updateSanctionedCountry(string memory _country, bool _status) =
public onlyOwner {
        sanctionedCountries[_country] =3D _status;
        emit SanctionCountryUpdated(_country, _status);
    }

    function initiatePayment(address _recipient, uint _amount, string memor=
y _currency, string memory _country) public {
        require(!sanctionedAddresses[msg.sender], "Sender is sanctioned.");
        require(!sanctionedAddresses[_recipient], "Recipient is sanctioned.=
